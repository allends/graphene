{
  "name": "@graphene/cli",
  "version": "0.0.1",
  "description": "Graphene CLI for Git branch management",
  "module": "index.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "graphene": "./dist/index.js"
  },
  "scripts": {
    "dev": "bun run --bun index.ts",
    "start": "bun run index.ts",
    "build": "bun build ./index.ts --outdir ./dist --target bun"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@graphene/core": "workspace:*",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "inquirer": "^12.4.1",
    "keytar": "^7.9.0",
    "open": "^10.1.0"
  }
}
